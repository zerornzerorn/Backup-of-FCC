function palindrome(str) {
  // Good luck!
   
  str  = str.toLowerCase();
  
  
  var temp= [];
  
  temp = str.split('');
  
  temp = temp.filter(function(val){
    
   if((val <= 'z' && val >= 'a') ||( val <= '9' && val >= '0') ){
      
     return val;
    }
  });
  
  str = temp.join('');
  
  var tempReverse = Array.from(temp);
  
  
    tempReverse.reverse();    
  
  var str1 = temp.join('');
  
  var str2 = tempReverse.join('');
  
  
  if (str1==str2) {
    
    return true;
  } else
    
  return false;
}

palindrome("fpdspflfGTREW^$#Tretp43fsa");











