/* method 3
function palindrome(str) {
  // Good luck!
  
  str = str.toLowerCase();
  
  var newStr = str.replace(/ /g, "");
  
  var arr = newStr.split('');
  
  var newArr=[];
  
  for( i = 0; i < arr.length; i ++){
    
    if (arr[i]>=0 && arr[i]<=9){
      
      newArr.push(arr[i]);
      
    } else if (arr[i]>='a' && arr[i]<='z'){
      
      newArr.push(arr[i]);
      
    }
  }
   
  arr=newArr;
  
  for (j = 0; j < arr.length && arr[j] === arr[arr.length-j-1]; j++){      
    
  }
    
  if (j === arr.length){
    
    return true;
  }
  return false;
  
}

palindrome("A man, a plan, a canal. Panama");

