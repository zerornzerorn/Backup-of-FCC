function palindrome(str) {
  // Good luck!
   
  str  = str.toLowerCase();
  
  var temp1 = [];
  var temp2 = [];
  
  temp1 = str.split('');
  
  temp1 = temp1.filter(function(val){
    
   if((val <= 'z' && val >= 'a') ||( val <= '9' && val >= '0') ){
      
     return val;
    }
  });
  
  for(var i = 0; i <temp1.length; i ++){
    
   temp2.push(temp1[temp1.length-i-1]);
  }
  
  
  var str1 = temp1.join('');
  

  
  var str2 = temp2.join('');  
  
  if (str1 === str2) {
    
    return true;
  } else
    
  return false;
}

