function largestOfFour(arr) {
  
  var length = arr.length;
  
  
  for (i = 0; i < arr.length; i ++){
    
     var temp = 0;
    
    for (j = 0; j < arr[i].length; j ++){
      
     
    
      if( temp < arr[i][j]){
        
        temp = arr[i][j];
      }
      
    }
      
      arr[i]= temp;
        
    
  }
  
  // You can do this!
  return arr;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
